<?xml version="1.0" encoding="UTF-8"?>
<config>
    <application
        title="Opérations d'aménagement publiques conduites par la ville de  Rennes et par Rennes Métropole"
        logo="https://public.sig.rennesmetropole.fr/ressources/img/logos/rm/logo_mviewer_transp.png"
        titlehelp="À propos des cartes thématiques" 
        help="apps/story/html/site_internet_help.html"
        showhelp="false"
        exportpng="false"
        style="apps/story/css/projets_urba.css"
        measuretools="false" 
        togglealllayersfromtheme="false" />
    
    <mapoptions maxzoom="20" minzoom="10" projection="EPSG:3857" center="-183500,6126800" zoom="12" />
    
    <baselayers style="default"><!-- style="default"||gallery" -->     
	    <baselayer visible="true" id="pvcisimple" thumbgallery="apps/site_internet/img/basemap/pvcisimple.jpg" title="Rennes Metropole" label="Plan de ville" type="WMTS" url="https://public.sig.rennesmetropole.fr/geowebcache/service/wmts?service/wmts?" layers="ref_fonds:pvci_simple_gris" format="image/png" style="_null" matrixset="EPSG:3857" fromcapacity="false" attribution="&lt;a href=&quot;https://applications.region-bretagne.fr/geonetwork/?uuid=3a0ac2e3-7af1-4dec-9f36-dae6b5a8c731&quot; target=&quot;_blank&quot; &gt;Rennes Métropole&lt;/a&gt;" maxzoom="22"  maxscale="1000" ></baselayer>
        <baselayer visible="false" id="ortho2017" thumbgallery="apps/site_internet/img/basemap/ortho2014.jpg" title="Rennes Metropole" label="Vue aérienne" type="WMTS" url="https://public.sig.rennesmetropole.fr/geowebcache/service/wmts?" layers="raster:ortho2017" format="image/jpeg" style="_null" matrixset="EPSG:3857" fromcapacity="false" attribution="&lt;a href=&quot;https://applications.region-bretagne.fr/geonetwork/?uuid=3a0ac2e3-7af1-4dec-9f36-dae6b5a8c731&quot; target=&quot;_blank&quot; &gt;Rennes Métropole&lt;/a&gt;" maxzoom="22"  maxscale="1000" ></baselayer>
    </baselayers>

    <searchparameters bbox="false" localities="false" features="false" static="false"/>
    
    <extensions>
         <extension type="component" id="carousel_photo" path="apps/public/addons"/>
         <extension type="javascript" src="apps/story/addons/multihighlight.js"/> 
    </extensions>
    
    <themes mini="true">
        <theme id="theme-zac" name="ZAC et ZA" collapsed="true" icon="fas fa-border-style"> 
            <layer 
                id="urba_fonc:zac"
                name="Zones d'aménagement concerté (ZAC) / zones d'activités (ZA)"
                type="wms"
                url="https://public.sig.rennesmetropole.fr/geoserver/wms"
                sld="https://public.sig.rennesmetropole.fr/ressources/img/mviewer/sld/zac_contour_encours.sld"
                legendurl="apps/story/img/legende_zac.png"
                visible="true"
                infoformat="application/vnd.ogc.gml"
                metadata="undefined"
                metadata-csw="https://public.sig.rennesmetropole.fr/geonetwork/srv/fre/catalog.search#/metadata/46a39b8c-5ec4-449a-bfe1-8410d0b76c84"
                queryable="false"
            infopanel="right-panel">
            </layer> 
        </theme>    
        <theme id="theme-communes" name="Projets et Communes" collapsed="true" icon="fas fa-map-marker-alt"> 
            
            <layer 
                id="projurbains_communes"
                name="Mairies / Hôtels de ville"
                type="customlayer"
                url="apps/story/customlayers/projets_urbains_communes.js" 
                style=""
                legendurl="apps/story/img/legend_communes.png"
                opacity="0.80"
                visible="true"
                queryable="true"
                featurecount="3"
                infopanel="right-panel">
                <template url="apps/story/templates/communes.mst"></template>
            </layer> 
            
            
            <layer 
                id="projurbains"
                name="Projets urbains"
                type="customlayer"
                url="apps/story/customlayers/projets_urbains.js" 
                style=""
                legendurl="apps/story/img/legend_pj_urbains.png"
                opacity="1"
                visible="true"
                tooltip="true" 
                tooltipenabled="true" 
                tooltipcontent="&lt;span class=&apos;rm-tooltip-title&apos;&gt;{{nom_ope}}&lt;/span&gt;"
                queryable="true"
                featurecount="3"
                infopanel="right-panel">
                <template url="apps/story/templates/projets_urba.mst"></template>
            </layer> 
        </theme> 
    </themes>
</config>
