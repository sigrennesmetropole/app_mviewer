<?xml version="1.0" encoding="UTF-8"?>
<config>
    <application
        title="PLUi Rennes Métropole"
        logo="https://public.sig.rennesmetropole.fr/ressources/img/logos/rm/logo_mviewer_gris_inverse.png"
        titlehelp="Carte interactive du PLUi de Rennes Métropole" 
        help="apps/PLUi/html/plui_help.html"
        showhelp="true"
        exportpng="false"
        style="apps/PLUi/css/carte_plui.css"
        coordinates="false"
        measuretools="false"
        togglealllayersfromtheme="false"
        openMultipleTheme="true"
        tutorial="true"
        tutorialFile="apps/PLUi/addons/data/didacticielPlui.json"
        showClickNbItems="false"
    />
    <!-- searchCadastre="true" -->
    <mapoptions maxzoom="20" minzoom="11" projection="EPSG:3857" center="-176970.72,6128725.98" zoom="11" />
    <baselayers style="default">
        <baselayer visible="true" id="PLUi" thumbgallery="apps/public/img/basemap/pvcisimple.jpg" title="Rennes Metropole" label="Fond de plan PLUi" type="WMTS" url="https://public.sig.rennesmetropole.fr/geowebcache/service/wmts?" layers="ref_fonds:plui_fond_plan" format="image/png" style="_null" matrixset="EPSG:3857" fromcapacity="false" attribution="&lt;a href=&quot;https://public.sig.rennesmetropole.fr/geonetwork/srv/fre/catalog.search#/home&quot; target=&quot;_blank&quot; &gt;Rennes Métropole&lt;/a&gt;"  ></baselayer> 
        <baselayer visible="false" id="ortho2017" thumbgallery="apps/public/img/basemap/ortho2014.jpg" title="Rennes Metropole" label="Vue aérienne" type="WMTS" url="https://public.sig.rennesmetropole.fr/geowebcache/service/wmts?" layers="raster:ortho2017" format="image/jpeg" style="_null" matrixset="EPSG:3857" fromcapacity="false" attribution="&lt;a href=&quot;https://public.sig.rennesmetropole.fr/geonetwork/srv/fre/catalog.search#/home&quot; target=&quot;_blank&quot; &gt;Rennes Métropole&lt;/a&gt;"  ></baselayer>
    </baselayers>

    <searchparameters bbox="false" localities="false" features="false" static="false" querymaponclick="true"
        searchRM="true" searchCadastre="true" 
        searchRMConf="apps/PLUi/addons/data/searchAddressConf.json" 
        disableSearchExtentLimitation="true"
        inputlabel="Adresse" />
    
    <themes mini="true"> 
        <theme id="theme-20200130112616" name="Droit de préemption" collapsed="false" icon="far fa-map">
            <layer
                id="dp"
                name="Droit de préemption simple"
                type="wms"
                url="https://public.sig.rennesmetropole.fr/geoserver/urba_fonc/ows"
                visible="true"
                infoformat="application/vnd.ogc.gml"
                metadata="undefined"
                metadata-csw="https://public.sig.rennesmetropole.fr/geonetwork/srv/fre/catalog.search#/metadata/6db35b05-e8bc-4339-a49b-828da0d3fd9f"
                queryable="false"
                filter="ETAPE = 'En cours' AND ARCHIVE = false AND type_dp = 'DPU simple'"
                infopanel="right-panel">
            </layer>
            <layer
                id="dp"
                name="Droit de préemption renforcé"
                type="wms"
                url="https://public.sig.rennesmetropole.fr/geoserver/urba_fonc/ows"
                visible="true"
                infoformat="application/vnd.ogc.gml"
                metadata="undefined"
                metadata-csw="https://public.sig.rennesmetropole.fr/geonetwork/srv/fre/catalog.search#/metadata/6db35b05-e8bc-4339-a49b-828da0d3fd9f"
                queryable="false"
                filter="ETAPE = 'En cours' AND ARCHIVE = false AND type_dp = 'DPU renforcé'"
                infopanel="right-panel">
            </layer>
        </theme>
    </themes>
</config>
