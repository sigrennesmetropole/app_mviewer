{{#features}}
	<li class="item">
		<h5 class="rm-popup-title-feature">{{nom}}</h5>
		<p class="rm-popup-text-feature">
			{{#adresse}}<span class='rm-popup-label'> ADRESSE :</span> {{adresse}} <br/>{{/adresse}}
			{{#type}}<span class='rm-popup-label'> TYPE :</span> {{{type}}}<br/>{{/type}}
			{{#nombre}}<span class='rm-popup-label'> NOMBRE D'EMPLACEMENTS :</span> {{nombre}}<br/>{{/nombre}}
			{{#gestionnaire}}<span class='rm-popup-label'> GESTIONNAIRE :</span> {{gestionnaire}}<br/>{{/gestionnaire}}
		</p>
	</li>
{{/features}}
