<?xml version="1.0" encoding="UTF-8"?>
<config>
    <application
        title="Données des antennes et capteurs electromagnétiques de l'ANFR"
        logo="apps/public/img/logo/logo_mviewer_transp.png"
        titlehelp="À propos des cartes thématiques"
        help="apps/site_internet/html/site_internet_help.html"
        showhelp="false"
        exportpng="false"
        style="apps/site_internet/css/site_internet_gris.css"
        measuretools="false"
        togglealllayersfromtheme="false"
         />

    <mapoptions maxzoom="20" minzoom="11" projection="EPSG:3857" center="-188244.25943429657,6125209.881604007" zoom="12" />

    <baselayers style="default">
        <baselayer visible="true" id="pvcisimple" thumbgallery="apps/public/img/basemap/pvcisimple.jpg" title="Rennes Metropole" label="Plan de ville" type="WMTS" url="https://public.sig.rennesmetropole.fr/geowebcache/service/wmts?service/wmts?" layers="ref_fonds:pvci_simple_gris" format="image/png" style="_null" matrixset="EPSG:3857" fromcapacity="false" attribution="&lt;a href=&quot;https://public.sig.rennesmetropole.fr/geonetwork/srv/fre/catalog.search#/metadata/46a39b8c-5ec4-449a-bfe1-8410d0b76c84&quot; target=&quot;_blank&quot; &gt;Rennes Métropole&lt;/a&gt;" maxzoom="22"  maxscale="1000" ></baselayer>
        <baselayer visible="false" id="ortho2020" thumbgallery="apps/public/img/basemap/ortho2014.jpg" title="Rennes Metropole" label="Vue aérienne" type="WMTS" url="https://public.sig.rennesmetropole.fr/geowebcache/service/wmts?" layers="raster:ortho2020" format="image/jpeg" style="_null" matrixset="EPSG:3857" fromcapacity="false" attribution="&lt;a href=&quot;https://public.sig.rennesmetropole.fr/geonetwork/srv/fre/catalog.search#/metadata/46a39b8c-5ec4-449a-bfe1-8410d0b76c84&quot; target=&quot;_blank&quot; &gt;Rennes Métropole&lt;/a&gt;" maxzoom="22"  maxscale="1000" ></baselayer>
    </baselayers>

    <extensions>
      <extension type="component" id="searchRM" path="apps/public/addons" configFile="/apps/public/addons/searchRM/conf/searchConf_RVA.json"/>
      <extension type="component" id="GUICustom" path="apps/public/addons"/>
    </extensions>

    <themes mini="true">
        <theme id="antennes_rennes" name="Antennes electromagnetiques" collapsed="true" icon="fas fa-broadcast-tower">
            <layer
                id="observatoire_anfr"
                name="Antennes de téléphonie"
                visible="true"
                type="customlayer"
                legendurl="apps/site_internet/img/legend/jeu_enfant_vdr.png"
                opacity="1"
                url="apps/site_internet/customlayer/observatoire_anfr.js"
                attribution=""
                metadata="undefined"
                metadata-csw=""
                queryable="true"
                featurecount="10"
                infopanel="right-panel">
            </layer>
        </theme>
        <theme id="capteurs_rennes" name="Capteurs permanents" collapsed="true" icon="fas fa-thermometer">
            <layer
                id="capteurs_exem"
                name="Capteurs permanents"
                visible="true"
                type="customlayer"
                legendurl="apps/site_internet/img/legend/jeu_enfant_vdr.png"
                opacity="1"
                url="apps/site_internet/customlayer/capteurs_exem.js"
                attribution=""
                metadata="undefined"
                metadata-csw=""
                queryable="true"
                featurecount="10"
                infopanel="right-panel">
            </layer>
        </theme>
    </themes>
</config>
